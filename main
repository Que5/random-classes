
class Cars:
    def __init__(self, name, make, model):
        self.name = name
        self.make = make
        self.model = model
        


asian = Cars("Toyota", "Hilux", 2015)
american = Cars("Ford", "Raptor", 2024)
european = Cars("VW", "Golf 8", 2024)

print(asian.name, asian.make, asian.model)
print(american.model)
print(european.make)
